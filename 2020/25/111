# 11537512
# too low
FILENAME = "demo.txt"  # expected 14897079
FILENAME = "input.txt"

BIG_NUMBER = 20201227
SUBJECT = 7


def transform(value: int, cycles: int) -> int:
    result = value
    for _ in range(cycles):
        result = (result * SUBJECT) % BIG_NUMBER
    return result


def detransform(n: int) -> int:
    value = 1
    i = 0
    while True:
        value = transform(value, 1)
        i += 1
        if value == n:
            return i


def main() -> None:
    result = 0
    with open(FILENAME, "r") as file:
        card_public_key = int(file.readline())
        door_public_key = int(file.readline())
    print(f"card_public_key = {card_public_key}")
    print(f"door_public_key = {door_public_key}")

    print(detransform(card_public_key))
    print(detransform(door_public_key))
    result = card_public_key
    for i in range(detransform(door_public_key)):
        result = (result * SUBJECT) % BIG_NUMBER
    print(result)
    result = door_public_key
    for i in range(detransform(card_public_key)):
        result = (result * SUBJECT) % BIG_NUMBER
    print(result)
    # print(transform(card_public_key, 7))
    # print(transform(card_public_key, 8))
    # print(transform(card_public_key, 9))
    # print(transform(card_public_key, 10))
    # print(transform(card_public_key, 11))
    # print(transform(card_public_key, 12))


if __name__ == "__main__":
    main()
